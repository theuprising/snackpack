{
  "name": "snackpack",
  "version": "4.0.3",
  "files": [
    "dist"
  ],
  "main": "./dist/node.js",
  "browser": "./dist/browser.js",
  "license": "MIT",
  "scripts": {
    "install:types": "flow-typed install",
    "test": "snackstack test",
    "repl": "snackstack repl",
    "docs": "snackstack docs",
    "build": "snackstack build",
    "postinstall": "yarn-bin-fix",
    "prepublish": "npm run test; npm run build"
  },
  "pre-commit": "test",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "docs",
      "flow-typed",
      "type-definitions"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      [
        "latest",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-promise-to-bluebird": "^1.1.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "commander": "^2.9.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.1.0",
    "happypack": "^2.2.1",
    "json-loader": "^0.5.4",
    "jsx": "^0.9.89",
    "jsx-control-statements": "^3.1.5",
    "lodash": "^4.16.3",
    "morgan": "^1.7.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "postcss-scss": "^0.3.1",
    "postcss-smart-import": "^0.4.3",
    "precss": "https://github.com/jboelen/precss.git",
    "pug": "^2.0.0-beta6",
    "pug-html-loader": "https://github.com/theuprising/pug-static-loader.git",
    "ramda": "^0.22.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "source-map-support": "^0.4.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.1.0-beta.8",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta12",
    "flow": "^0.2.3",
    "flow-typed": "^2.0.0",
    "snackstack": "0.0.12",
    "standard": "^8.6.0",
    "yarn-bin-fix": "^0.1.15"
  }
}
